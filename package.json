{
  "name": "eslint-config-xs",
  "version": "1.6.3",
  "description": "ESLint and Prettier configuration for NodeJS or ReactJS projects, either Javascript or Typescript based",
  "author": "Damiano Barbati (https://github.com/damianobarbati)",
  "repository": "https://github.com/damianobarbati/eslint-config-xs",
  "engines": {
    "node": ">=16"
  },
  "main": "default/index.js",
  "license": "MIT",
  "keywords": [
    "eslint",
    "prettier",
    "javascript",
    "typescript",
    "react"
  ],
  "scripts": {
    "test": "yarn test:default && yarn test:node && yarn test:node-ts && yarn test:react && yarn test:react-ts",
    "test:default": "cd default && node *.spec.js",
    "test:node": "cd node && node *.spec.js",
    "test:node-ts": "cd node-ts && node *.spec.js",
    "test:react": "cd react && node *.spec.js",
    "test:react-ts": "cd react-ts && node *.spec.js"
  },
  "dependencies": {
    "@babel/core": "^7.21.8",
    "@babel/eslint-parser": "^7.21.8",
    "@babel/plugin-syntax-import-assertions": "^7.20.0",
    "@babel/preset-env": "^7.21.5",
    "@babel/preset-react": "^7.18.6",
    "@babel/preset-typescript": "^7.21.5",
    "@typescript-eslint/eslint-plugin": "^5.59.5",
    "@typescript-eslint/parser": "^5.59.5",
    "eslint": "^8.40.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-babel": "^5.3.1",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-jest": "^27.2.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.32.1",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-testing-library": "^5.11.0",
    "prettier": "^2.8.8",
    "prettier-plugin-packagejson": "^2.4.0",
    "prettier-plugin-tailwindcss": "^0.2.8",
    "stylelint": "^15.6.1",
    "stylelint-config-prettier": "^9.0.4",
    "stylelint-config-sass-guidelines": "^10.0.0",
    "stylelint-config-standard": "^33.0.0",
    "stylelint-prettier": "^3.0.0",
    "typescript": "^5.0.4"
  }
}

{
  "name": "eslint-config-xs",
  "version": "1.2.1",
  "description": "ESLint rules for JavaScript projects",
  "author": "Damiano Barbati (https://github.com/damianobarbati)",
  "repository": "https://github.com/damianobarbati/eslint-config-xs",
  "engines": {
    "node": ">=16"
  },
  "main": "default/index.js",
  "license": "MIT",
  "keywords": [
    "eslint",
    "javascript",
    "react",
    "eslintconfig"
  ],
  "scripts": {
    "test": "yarn test:default && yarn test:node && yarn test:node-ts && yarn test:react",
    "test:default": "cd default && node *.spec.js",
    "test:node": "cd node && node *.spec.js",
    "test:node-ts": "cd node-ts && node *.spec.js",
    "test:react": "cd react && node *.spec.js"
  },
  "dependencies": {
    "@babel/core": "^7.18.9",
    "@babel/eslint-parser": "^7.18.9",
    "@babel/plugin-syntax-import-assertions": "^7.18.6",
    "@babel/preset-env": "^7.18.9",
    "@babel/preset-react": "^7.18.6",
    "@babel/preset-typescript": "^7.18.6",
    "@typescript-eslint/eslint-plugin": "^5.31.0",
    "@typescript-eslint/parser": "^5.31.0",
    "eslint": "^8.20.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-babel": "^5.3.1",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jest": "^26.6.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.30.1",
    "eslint-plugin-react-hooks": "^4.6.0",
    "prettier": "^2.7.1",
    "typescript": "^4.7.4"
  }
}

{
  "name": "eslint-config-xs",
  "version": "1.5.3",
  "description": "ESLint rules for JavaScript projects",
  "author": "Damiano Barbati (https://github.com/damianobarbati)",
  "repository": "https://github.com/damianobarbati/eslint-config-xs",
  "engines": {
    "node": ">=16"
  },
  "main": "default/index.js",
  "license": "MIT",
  "keywords": [
    "eslint",
    "javascript",
    "react",
    "eslintconfig"
  ],
  "scripts": {
    "test": "yarn test:default && yarn test:node && yarn test:node-ts && yarn test:react && yarn test:react-ts",
    "test:default": "cd default && node *.spec.js",
    "test:node": "cd node && node *.spec.js",
    "test:node-ts": "cd node-ts && node *.spec.js",
    "test:react": "cd react && node *.spec.js",
    "test:react-ts": "cd react-ts && node *.spec.js"
  },
  "dependencies": {
    "@babel/core": "^7.20.2",
    "@babel/eslint-parser": "^7.19.1",
    "@babel/plugin-syntax-import-assertions": "^7.20.0",
    "@babel/preset-env": "^7.20.2",
    "@babel/preset-react": "^7.18.6",
    "@babel/preset-typescript": "^7.18.6",
    "@typescript-eslint/eslint-plugin": "^5.42.1",
    "@typescript-eslint/parser": "^5.42.1",
    "eslint": "^8.27.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-babel": "^5.3.1",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jest": "^27.1.5",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.31.10",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-testing-library": "^5.9.1",
    "prettier": "^2.7.1",
    "typescript": "^4.8.4"
  }
}
